{
    "name": "zoom-meeting-plus",
    "version": "1.0.0",
    "description": "",
    "main": ".eslintrc.js",
    "scripts": {
        "clean": "rimraf ../dist/*",
        "copy:resources": "mkdir -p ../dist && cp -r public/* ../dist/",
        "tsc": "tsc",
        "tsc:watch": "tsc -w",
        "build:outer": "webpack --config webpack.config.js",
        "build:inner": "webpack --config webpack.config.inner.js",
        "build": "run-s clean copy:resources build:inner build:outer ",
        "start:backend": "node backend/server.js",
        "start:outer": "webpack serve --config webpack.config.js",
        "start:pre": "run-s build start:outer",
        "start": "run-p start:backend start:pre",
        "start2": "run-p copy:resources tsc:watch start:outer start:backend",
        "deploy:backend": "git push heroku master",
        "heroku-postbuild": "echo \"nobuild in heroku(w.o.)\"",
        "build:docker": "date +%Y%m%d%H%M%S > dummy && DOCKER_BUILDKIT=1 docker build --ssh default  . -t zoom-meeting-plus",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/w-okada/zoom-meeting-plus.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
