    "main": ".eslintrc.js",
    "scripts": {
        "clean": "rimraf ../docs/*",
        "copy:resources": "mkdir -p ../docs && cp -r public/* ../docs/",
        "tsc": "tsc",
        "tsc:watch": "tsc -w",
        "build:outer": "webpack --config webpack.config.js",
        "build:inner": "webpack --config webpack.config.inner.js",
        "build": "run-s clean copy:resources tsc build:inner build:outer ",
        "start:backend": "node ../backend/server.js",
        "start:outer": "webpack serve --config webpack.config.js",
        "start:pre": "run-s build start:outer",
        "start": "run-p start:backend start:pre",
        "start2": "run-p copy:resources tsc:watch start:outer start:backend",
        "heroku-postbuild": "echo \"nobuild in heroku(w.o.)\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
